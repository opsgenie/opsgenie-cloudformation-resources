{
  "typeName": "Atlassian::Opsgenie::User",
  "description": "Opsgenie User",
  "sourceUrl": "https://github.com/opsgenie/opsgenie-cloudformation-resources",
  "definitions": {
  },
  "properties": {
    "Id": {
      "type": "string"
    },
    "OpsgenieApiEndpoint": {
      "type": "string",
      "pattern": "^https:\/\/api(\\.eu|\\.sandbox|)\\.opsgenie\\.com$",
      "minLength": 1
    },
    "OpsgenieApiKey": {
      "type": "string",
      "pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"
    },
    "Username": {
      "type": "string",
      "description": "Opsgenie Username the mail address of the user",
      "minLength": 1
    },
    "FullName": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9- _.]+$",
      "minLength": 1,
      "description": "User full name"
    },
    "Role": {
      "type": "string",
      "minLength": 1,
      "description": "User role, default is User"
    }
  },
  "required": [
    "OpsgenieApiEndpoint",
    "Username",
    "FullName",
    "Role",
    "OpsgenieApiKey"
  ],
  "createOnlyProperties": [
    "/properties/Id"
  ],
  "writeOnlyProperties": [
    "/properties/OpsgenieApiKey",
    "/properties/OpsgenieApiEndpoint"
  ],
  "primaryIdentifier": [
    "/properties/Id"
  ],
  "handlers": {
    "create": {
      "permissions": [
        ""
      ]
    },
    "read": {
      "permissions": [
        ""
      ]
    },
    "update": {
      "permissions": [
        ""
      ]
    },
    "delete": {
      "permissions": [
        ""
      ]
    },
    "list": {
      "permissions": [
        ""
      ]
    }
  }
}

